{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "palette": [
                {
                    "color": "#ffffff",
                    "name": "Base",
                    "slug": "base"
                },
                {
                    "color": "#000000",
                    "name": "Contrast",
                    "slug": "contrast"
                },
                {
                    "color": "#16425B",
                    "name": "Indigo Dye",
                    "slug": "accent-1"
                },
                {
                    "color": "#2F6690",
                    "name": "Lapis Lazuli",
                    "slug": "accent-2"
                },
                {
                    "color": "#82B4D3",
                    "name": "Carolina Blue",
                    "slug": "accent-3"
                },
                {
                    "color": "#235E70",
                    "name": "Midnight Green",
                    "slug": "accent-4"
                },
                {
                    "color": "#30A2C5",
                    "name": "Pacific Cyan",
                    "slug": "accent-5"
                },
                {
                    "color": "#CDE7EF",
                    "name": "Columbia Blue",
                    "slug": "accent-6"
                },
                {
                    "color": "#f4f4f4",
                    "name": "Base Midtone",
                    "slug": "base-midtone"
                },
                {
                    "color": "#323232",
                    "name": "Contrast Midtone",
                    "slug": "contrast-midtone"
                }
            ]
        },
        "custom": {
            "fontSizeHuge": "var(--wp--preset--font-size--huge)",
            "spacing--10": "clamp(0.4375rem, calc(0.375rem + 0.37vw), 0.625rem)"
        },
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1340px"
        },
        "spacing": {
            "defaultSpacingSizes": false,
            "spacingSizes": [
                {
                    "name": "X-Small",
                    "size": "clamp(0.625rem, calc(0.5rem + 0.37vw), 0.875rem)",
                    "slug": "20"
                },
                {
                    "name": "Small",
                    "size": "clamp(0.9375rem, calc(0.8125rem + 0.37vw), 1.25rem)",
                    "slug": "30"
                },
                {
                    "name": "Regular",
                    "size": "clamp(1.25rem, calc(1.125rem + 0.61vw), 1.75rem)",
                    "slug": "40"
                },
                {
                    "name": "Large",
                    "size": "clamp(1.75rem, calc(1.5rem + 0.61vw), 2.5rem)",
                    "slug": "50"
                },
                {
                    "name": "X-Large",
                    "size": "clamp(2.1875rem, calc(1.9375rem + 1.14vw), 3.5rem)",
                    "slug": "60"
                },
                {
                    "name": "2X-Large",
                    "size": "clamp(3rem, calc(2.5rem + 1.83vw), 5rem)",
                    "slug": "70"
                },
                {
                    "name": "3X-Large",
                    "size": "clamp(4rem, calc(3.25rem + 2.44vw), 7rem)",
                    "slug": "80"
                }
            ],
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "dvh"
            ]
        },
        "typography": {
            "defaultFontSizes": false,
            "dropCap": true,
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "System Font",
                    "slug": "system-font"
                },
                {
                    "fontFace": [
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Ubuntu Sans",
                            "fontStyle": "normal",
                            "fontWeight": "100 800",
                            "src": [
                                "file:./assets/fonts/ubuntu-sans/ubuntu-sans-variable.woff2"
                            ]
                        },
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Ubuntu Sans",
                            "fontStyle": "italic",
                            "fontWeight": "100 800",
                            "src": [
                                "file:./assets/fonts/ubuntu-sans/ubuntu-sans-italic-variable.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "'Ubuntu Sans', Seravek, 'Gill Sans Nova', Ubuntu, Calibri, 'DejaVu Sans', source-sans-pro, sans-serif",
                    "name": "Ubuntu Sans",
                    "slug": "ubuntu-sans"
                },
                {
                    "fontFace": [
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Playfair Display",
                            "fontStyle": "normal",
                            "fontWeight": "400 900",
                            "src": [
                                "file:./assets/fonts/playfair-display/playfair-display-variable.woff2"
                            ]
                        },
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Playfair Display",
                            "fontStyle": "italic",
                            "fontWeight": "400 900",
                            "src": [
                                "file:./assets/fonts/playfair-display/playfair-display-italic-variable.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "'Playfair Display', Superclarendon, 'Bookman Old Style', 'URW Bookman', 'URW Bookman L', 'Georgia Pro', Georgia, serif",
                    "name": "Playfair Display",
                    "slug": "playfair-display"
                },
                {
                    "fontFace": [
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Roboto Serif",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "100 900",
                            "src": [
                                "file:./assets/fonts/roboto-serif/roboto-serif-variable.woff2"
                            ]
                        },
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Roboto Serif",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "100 900",
                            "src": [
                                "file:./assets/fonts/roboto-serif/roboto-serif-italic-variable.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "'Roboto Serif', serif",
                    "name": "Roboto Serif",
                    "slug": "roboto-serif"
                },
                {
                    "fontFace": [
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Roboto Flex",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "100 1000",
                            "src": [
                                "file:./assets/fonts/roboto-flex/roboto-flex-variable.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "'Roboto Flex', system-ui, sans-serif",
                    "name": "Roboto Flex",
                    "slug": "roboto-flex"
                },
                {
                    "fontFace": [
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Roboto Flex Expanded",
                            "fontStretch": "150%",
                            "fontStyle": "normal",
                            "fontVariationSettings": "'wdth' 150",
                            "fontWeight": "100 1000",
                            "src": [
                                "file:./assets/fonts/roboto-flex/roboto-flex-variable.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "'Roboto Flex Expanded', system-ui, sans-serif",
                    "name": "Roboto Flex Expanded",
                    "slug": "roboto-flex-expanded"
                },
                {
                    "fontFace": [
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Roboto Flex Condensed",
                            "fontStretch": "25%",
                            "fontStyle": "normal",
                            "fontVariationSettings": "'wdth' 25",
                            "fontWeight": "100 1000",
                            "src": [
                                "file:./assets/fonts/roboto-flex/roboto-flex-variable.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "'Roboto Flex Condensed', system-ui, sans-serif",
                    "name": "Roboto Flex Condensed",
                    "slug": "roboto-flex-condensed"
                },
                {
                    "fontFace": [
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Nunito",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/nunito/nunito-variable.woff2"
                            ]
                        },
                        {
                            "fontDisplay": "swap",
                            "fontFamily": "Nunito",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/nunito/nunito-italic-variable.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "Nunito, ui-rounded, 'Hiragino Maru Gothic ProN', Quicksand, Comfortaa, Manjari, 'Arial Rounded MT', 'Arial Rounded MT Bold', Calibri, source-sans-pro, sans-serif",
                    "name": "Nunito",
                    "slug": "nunito"
                }
            ],
            "fontSizes": [
                {
                    "fluid": false,
                    "name": "Small",
                    "size": "0.813rem",
                    "slug": "small"
                },
                {
                    "fluid": {
                        "max": "1em",
                        "min": "0.875rem"
                    },
                    "name": "Medium",
                    "size": "1em",
                    "slug": "medium"
                },
                {
                    "fluid": {
                        "max": "1.25em",
                        "min": "1.125rem"
                    },
                    "name": "Large",
                    "size": "1.25em",
                    "slug": "large"
                },
                {
                    "fluid": {
                        "max": "1.75em",
                        "min": "1.5rem"
                    },
                    "name": "Extra Large",
                    "size": "1.75em",
                    "slug": "x-large"
                },
                {
                    "fluid": {
                        "max": "4em",
                        "min": "3.5rem"
                    },
                    "name": "Extra Extra Large",
                    "size": "3.5em",
                    "slug": "xx-large"
                }
            ],
            "textColumns": true,
            "writingMode": true
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "blocks": {
            "core/archives": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/avatar": {
                "border": {
                    "radius": "1000px"
                }
            },
            "core/calendar": {
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                },
                "elements": {
                    "link": {
                        ":hover": {
                            "color": {
                                "text": "var(--wp--preset--color--accent-1)"
                            }
                        },
                        "color": {
                            "text": "var(--wp--preset--color--contrast)"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontStyle": "normal",
                    "fontWeight": "500",
                    "lineHeight": "1.8"
                }
            },
            "core/code": {
                "color": {
                    "background": "var(--wp--preset--color--base-midtone)"
                },
                "spacing": {
                    "padding": {
                        "bottom": "var(--wp--preset--spacing--20)",
                        "left": "var(--wp--preset--spacing--20)",
                        "right": "var(--wp--preset--spacing--20)",
                        "top": "var(--wp--preset--spacing--20)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/latest-posts": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/list": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/list-item": {
                "spacing": {
                    "margin": {
                        "top": "0.8rem"
                    }
                }
            },
            "core/page-list": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/paragraph": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/post-excerpt": {
                "color": {
                    "text": "var(--wp--preset--color--contrast-midtone)"
                }
            },
            "core/post-terms": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/separator": {
                "css": " &:not(.is-style-wide):not(.is-style-dots):not(.alignwide):not(.alignfull){width: 120px}"
            },
            "core/verse": {
                "typography": {
                    "lineHeight": "1.6"
                }
            },
            "woocommerce/product-categories": {
                "typography": {
                    "lineHeight": "1.5"
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--contrast)"
        },
        "elements": {
            "button": {
                ":hover": {
                    "color": {
                        "background": "color-mix( in lab, currentColor, var(--wp--preset--color--accent-1) 70% )",
                        "text": "var(--wp--preset--color--base)"
                    }
                },
                "color": {
                    "background": "var(--wp--preset--color--accent-1)",
                    "text": "var(--wp--preset--color--base)"
                }
            },
            "caption": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                },
                "typography": {
                    "lineHeight": "1.1"
                }
            },
            "link": {
                ":active": {
                    "typography": {
                        "textDecoration": "none"
                    }
                },
                ":focus": {
                    "typography": {
                        "textDecoration": "underline dashed"
                    }
                },
                ":hover": {
                    "color": {
                        "text": "color-mix( in lab, currentColor, var(--wp--preset--color--accent-2) 70% )"
                    },
                    "typography": {
                        "textDecoration": "underline"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                },
                "typography": {
                    "textDecoration": "underline"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--40)",
            "padding": {
                "bottom": "0px",
                "left": "var(--wp--preset--spacing--50)",
                "right": "var(--wp--preset--spacing--50)",
                "top": "0px"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--roboto-flex)"
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        },
        {
            "area": "header",
            "name": "header-contrast",
            "title": "Header contrast"
        },
        {
            "area": "header",
            "name": "header-with-topbar",
            "title": "Header with topbar"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        },
        {
            "area": "footer",
            "name": "footer-big",
            "title": "Footer big"
        },
        {
            "area": "footer",
            "name": "footer-with-cta",
            "title": "Footer with CTA"
        },
        {
            "area": "footer",
            "name": "footer-with-contacts",
            "title": "Footer with contacts"
        }
    ]
}